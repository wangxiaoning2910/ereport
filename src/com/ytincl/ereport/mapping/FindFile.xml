<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ytincl.ereport.dao.FindFileDao">
	<resultMap type="FindFile" id="result_findfile"></resultMap>
	<select id="selectFindFileByName" parameterType="String" resultMap="result_findfile">
		select * from tobefindereport t where t.date = #{date} and t.name =#{name} order by id asc
	</select>	
	<select id="selectFindFileZYByName" parameterType="String" resultMap="result_findfile">
		select * from tobefindereport t where t.date = #{date} and t.name =#{name} order by id desc
	</select>	
</mapper>
